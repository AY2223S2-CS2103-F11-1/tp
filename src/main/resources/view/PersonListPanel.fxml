<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <TableView fx:id="table">
      <columns>
         <TableColumn fx:id="index" editable="false" style="-fx-alignment: CENTER;" minWidth="80.0" maxWidth="80.0" prefWidth="80.0" text="Id" />
         <TableColumn fx:id="name" editable="false" text="Name" />
         <TableColumn fx:id="photo" editable="false" style="-fx-alignment: CENTER;" text="Photo" />
         <TableColumn fx:id="telegram" text="Telegram/Phone" />
         <TableColumn fx:id="email" text="NUS Email" />
         <TableColumn fx:id="address" text="Address" />
         <TableColumn editable="false" text="Statistics">
            <columns>
               <TableColumn fx:id="performance" style="-fx-alignment: CENTER;" text="Performance" />
               <TableColumn fx:id="remark" style="-fx-alignment: CENTER;" text="Remark" />
            </columns>
         </TableColumn>
      </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
   </TableView>
</VBox>
