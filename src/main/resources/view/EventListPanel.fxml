<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.image.ImageView?>

<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <HBox prefWidth="400.0" VBox.vgrow="ALWAYS">
        <children>
         <VBox prefHeight="400.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
           <HBox>
           <Label id="tutorialLabel" alignment="CENTER" contentDisplay="CENTER" text="Tutorials" textFill="WHITE"/>
           <ImageView fx:id="tutorial"></ImageView>
           </HBox>
           <children>
                  <ListView fx:id="eventListViewLeftCol" prefHeight="6662.0" prefWidth="70.0">
                      <padding>
                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                      </padding>
                  </ListView>
            </children>
         </VBox>
          <VBox prefHeight="400.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
              <HBox>
                <Label id="labLabel" alignment="CENTER" contentDisplay="CENTER" text="Labs" textFill="WHITE" HBox.hgrow="ALWAYS" />
                <ImageView fx:id="lab"></ImageView>
              </HBox>
              <ListView fx:id="eventListViewMidCol" prefHeight="6662.0" prefWidth="70.0" HBox.hgrow="ALWAYS">
                <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
                <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </HBox.margin>
              </ListView>
            </children>
          </VBox>
          <VBox prefHeight="400.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
            <children>
              <HBox>
              <Label id="consultationLabel" alignment="CENTER" contentDisplay="CENTER" text="Consultations" textFill="WHITE" HBox.hgrow="ALWAYS" />
              <ImageView fx:id="consultation"></ImageView>
              </HBox>
              <ListView fx:id="eventListViewRightCol" prefHeight="6662.0" prefWidth="70.0" HBox.hgrow="ALWAYS">
                <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </HBox.margin>
                <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
              </ListView>
            </children>
          </VBox>
        </children>
    </HBox>
</VBox>
